{
    "name": "Hand Terminal",
    "version": "1.0",
    "description": "Live Gesture Control",
    "permissions": ["activeTab","declarativeContent", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
        "128": "images/hand_terminal128.png"
    }
    },
    "icons": {
    "128": "images/hand_terminal128.png"
    },
    "manifest_version": 2
  }